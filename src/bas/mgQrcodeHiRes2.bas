W = 280
H = 192
CLEN = 58
ROWS = INT(LEN(A$)/CLEN)
PIXELSIZE = 4

XOFFSET = INT((W - (CLEN * PIXELSIZE)) / 2)
YOFFSET = INT((H - (ROWS * PIXELSIZE)) / 2)

HCOLOR=3

FOR R = 0 TO ROWS-1
  FOR C = 1 TO CLEN
    INDEX = R * CLEN + C
    IF INDEX > LEN(A$) THEN GOTO 260
    CHAR$ = MID$(A$, INDEX, 1)
    IF CHAR$ = "#" THEN
      X = XOFFSET + (C - 1) * PIXELSIZE
      Y = YOFFSET + R * PIXELSIZE
      FOR DX = 0 TO PIXELSIZE-1
        FOR DY = 0 TO PIXELSIZE-1
          HPLOT X+DX,Y+DY
        NEXT DY : NEXT DX
    END IF
  NEXT C
NEXT R
END
